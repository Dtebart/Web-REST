@var i@
<table>
	<thead>
		<tr>
			<th> Artikelnummer </th>
			<th> Artikelname </th>
			<th> Preis </th>
		</tr>
	</thead>
	<tfoot>
	</tfoot>
	<tbody>
		@for i = 0; i< context.length; i++@
		@article_obj = context[i];@
		<tr>
			<td>#article_obj.number#</td>
			<td>#article_obj.name#</td>
			<td>#article_obj.price#</td>
		</tr>
		@endfor@
	</tbody>
</table>